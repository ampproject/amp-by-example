{
  "name": "playground",
  "version": "1.0.0",
  "description": "An interactive AMP playground.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.common.js",
    "start-backend": "dev_appserver.py app.yaml",
    "build": "rm -rf dist && webpack --config webpack.prod.js && cp -R static/* dist",
    "deploy": "npm run build && gcloud app deploy --version 1 --project amp-playground -q",
    "test": "babel-node spec/run.js && eslint src/**/*.js",
    "lint": "eslint src/**/*.js --fix"
  },
  "author": "Sebastian Benz",
  "license": "Apache-2.0",
  "devDependencies": {
    "absolutify": "0.1.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "codemirror": "5.39.2",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-plugin-google-camelcase": "0.0.2",
    "extract-text-webpack-plugin": "2.1.2",
    "firebase": "5.3.0",
    "firebase-admin": "5.12.0",
    "firebase-functions": "^2.0.5",
    "handlebars-loader": "1.7.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "2.30.1",
    "jasmine": "2.99.0",
    "js-beautify": "1.7.5",
    "keymaster": "1.6.2",
    "normalize.css": "7.0.0",
    "offline-plugin": "4.9.1",
    "script-ext-html-webpack-plugin": "1.8.8",
    "standard": "10.0.3",
    "style-ext-html-webpack-plugin": "3.4.7",
    "style-loader": "0.21.0",
    "url-loader": "0.5.9",
    "url-search-params": "0.9.0",
    "wade": "0.3.3",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.2",
    "webpack-merge": "4.1.2",
    "workbox-webpack-plugin": "^3.4.1"
  },
  "standard": {
    "globals": [
      "fetch",
      "URL"
    ]
  },
  "dependencies": {
    "copy-webpack-plugin": "^4.5.2",
    "handlebars": "^4.0.11",
    "hoek": "^5.0.4"
  }
}
